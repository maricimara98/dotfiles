[user]
    name = Jucimara Pereira
    email = maricimara98@outlook.com
[core]
    editor = micro
    autocrlf = input
    excludesfile = ~/.gitignore_global
[color]
    ui = auto
[init]
    defaultBranch = main
[alias]
    st = status
    co = checkout
    br = branch
    cm = commit -m
    unstage = reset HEAD --
    last = log -1 HEAD
    lg = log --oneline --graph --decorate --all
